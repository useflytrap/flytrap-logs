{
	"name": "@useflytrap/logs",
	"version": "0.0.7",
	"description": "Instant Stripe-level observability for your Next.js project",
	"repository": "useflytrap/flytrap-logs",
	"engines": {
		"node": ">=18"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs"
		}
	},
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"author": "Rasmus Gustafsson <rasmus@useflytrap.com>",
	"license": "MIT",
	"type": "module",
	"keywords": [
		"typescript",
		"logging",
		"canonical logging"
	],
	"scripts": {
		"build": "rollup -c",
		"test": "pnpm test:core && pnpm test:types",
		"test:core": "vitest run",
		"test:types": "tsc --noEmit",
		"test:coverage": "vitest run --coverage",
		"lint": "pnpm eslint --fix \"{src,test}/**/*.{js,json,ts}\"",
		"prepublishOnly": "pnpm lint",
		"release": "np"
	},
	"dependencies": {
		"serialize-error": "^11.0.3",
		"zod": "^3.22.4"
	},
	"peerDependencies": {
		"next": ">=13"
	},
	"devDependencies": {
		"@eslint/js": "^9.0.0",
		"@rollup/plugin-commonjs": "^25.0.7",
		"@rollup/plugin-node-resolve": "^15.2.3",
		"@types/node": "^18.16.0",
		"@typescript-eslint/eslint-plugin": "^7.0.2",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^8.10.0",
		"eslint-plugin-prettier": "^5.1.3",
		"globals": "^15.0.0",
		"np": "^10.0.3",
		"rollup": "^4.14.3",
		"rollup-plugin-dts": "^6.1.0",
		"rollup-plugin-esbuild": "^6.1.1",
		"typescript": "^5.3.3",
		"typescript-eslint": "^7.6.0",
		"vite": "^5.1.4",
		"vitest": "^1.3.1"
	}
}
